		quitMap.put(LabourHealthRegimentInsuranceEnum.ISHEALTHINSURANCE, ishealthinsurance == true ? "Y" : "N");
		quitMap.put(LabourHealthRegimentInsuranceEnum.ISLABOURRETREAT, islabourtreat == true ? "Y" : "N");
