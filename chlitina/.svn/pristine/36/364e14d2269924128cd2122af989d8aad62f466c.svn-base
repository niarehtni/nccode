				// glbdef22,健保费应缴金额
				defvo.setAttributeValue(
					"glbdef22",
					vo.getHealthstuffact()
						.add(vo.getLastmonthhealthstuffact() == null ? UFDouble.ZERO_DBL : vo
							.getLastmonthhealthstuffact())
						.add((UFDouble) defvo.getAttributeValue("glbdef22")));
			    }
			    // glbdef25,劳退投保天数
			    defvo.setAttributeValue("glbdef25", (vo.getRetiredays() == null ? 0 : vo.getRetiredays())
				    + (vo.getLastmonthretiredays() == null ? 0 : vo.getLastmonthretiredays())
