			selPsnPanel = new ConditionSelPsnPanel();
			selPsnPanel.setModel((AbstractUIAppModel) getModel());
			selPsnPanel.init();
		}
		return selPsnPanel;
	}
	public BUPanel getBuPanel() {
		if(buPanel==null){
			buPanel = new BUPanel();
			buPanel.setModel((AbstractUIAppModel) getModel());
			buPanel.init();
			buPanel.getBuRef().addValueChangedListener(this);
		}
		return buPanel;
	}


