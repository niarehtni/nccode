					}
				}

			}

			vos = service.queryByCondition(PsndocDefUtil.getPsnHealthVO().getClass(),
					" pk_psndoc='" + pk_psndoc + "' ");
			if (vos != null && vos.length > 0) {
				for (PsndocDefVO vo : vos) {
					if (vo == null || !vo.getLastflag().booleanValue()) {
						continue;// 寻找最新的投保记录
					}
					if (vo.getAttributeValue("insuranceform") != null
