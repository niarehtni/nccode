		<property name="btnName" value="生成"/>
		<property name="billForm"><ref bean="billForm"/></property>
		<property name="billListView"><ref bean="billListView"/></property>
		<property name="exceptionHandler"><ref bean="exceptionHandler" /></property>
	</bean>

	<!--======= 动作：[newActions] [a充保M{整] ===========-->
