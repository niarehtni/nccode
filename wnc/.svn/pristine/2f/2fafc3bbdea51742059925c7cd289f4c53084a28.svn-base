    /**
     * 提供一个精度设置方法
     * 
     * @return
     */
    public DecimalFormat getTimeRuleDecimalFormat() {
	DecimalFormat dcmFmt = new DecimalFormat("0.00");// 设置小数位数-默J@示晌
	Integer dec = getTimedecimal();
	if (dec != null) {
	    if (dec == 0) {
		dcmFmt = new DecimalFormat("0");
	    } else if (dec == 1) {
		dcmFmt = new DecimalFormat("0.0");
	    }
	}
	return dcmFmt;
    }

    public UFDouble getDecimalFormatDouble(UFDouble value) {
	return new UFDouble(getTimeRuleDecimalFormat().format(value.doubleValue()));
    }

    /**
     * 根据考勤规则中的小数位数和舍位方式 转换
     * 
     * @param value
     * @return
     */
    public UFDouble getTimeRuleUFDouble(UFDouble value) {
	try {
	    if (value == null)
		value = UFDouble.ZERO_DBL;
	    Integer digits = getTimedecimal();// 小数位数
	    Integer rounding = getRounding();// 舍位方式
	    UFDouble o = null;
	    if (rounding == TimeRuleConst.ROUNDING_UP) {// 向上取整
		o = value.setScale(digits, UFDouble.ROUND_UP);
	    } else if (rounding == TimeRuleConst.ROUNDING_DOWN) {// 向下取整
		o = value.setScale(digits, UFDouble.ROUND_DOWN);
	    } else {// 四舍五入
		o = value.setScale(digits, UFDouble.ROUND_HALF_UP);
	    }
	    return o;
	} catch (NumberFormatException e) {
	    Logger.warn("数据转换失败：" + value);
	    return value;
	}
    }

