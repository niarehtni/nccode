	protected String getRandomString(int length) {
		String seed = "0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ";
		String tail = "";
		for (int i = 0; i < length; i++) {
			int pos = (int) (Math.random() * seed.length());
			tail += seed.substring(pos, pos + 1);
		}
		return tail;
	}

