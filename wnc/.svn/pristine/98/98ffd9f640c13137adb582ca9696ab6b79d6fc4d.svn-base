	// 保存明表
	private void SaveNHIDetail() throws BusinessException {
		SimpleDocServiceTemplate service = new SimpleDocServiceTemplate("TWHRGLBDEF");
		List<PsndocDefVO> nhiTotalVOs = getNHIDetailSaveVOs();
		if (nhiTotalVOs != null && nhiTotalVOs.size() > 0) {
			for (PsndocDefVO vo : nhiTotalVOs) {
				// ssx added for Encryption
				// on 2018-11-07
				for (String fn : NhiCalcUtils.getNhiDetailEncryptionAttributes()) {
					UFDouble originValue = this.getUFDouble((UFDouble) vo.getAttributeValue(fn));
					double encryptedValue = SalaryEncryptionUtil
							.encryption(this.getUFDouble(originValue).doubleValue());
					vo.setAttributeValue(fn, new UFDouble(encryptedValue));
				}
				// end
				service.insert(vo);
			}
		}
	}

