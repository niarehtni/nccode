create table tbm_daystatbb
(
  calcperiod        CHAR(6),
  dr                NUMBER(10) default 0,
  hournum           NUMBER(16,4) not null,
  pk_daystat        CHAR(20) null,
  pk_daystatb       CHAR(20)  null,
  pk_group          CHAR(20)  null,
  pk_org            CHAR(20)  null,
  pk_timeitem       CHAR(20)  null,
  timeitemunit      NUMBER(38) default 2  null,
  ts                CHAR(19) default to_char(sysdate,'yyyy-mm-dd hh24:mi:ss')
)