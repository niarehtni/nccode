package nc.impl.hi.psndoc;

public class User {

	public String code=null;
	public String cardno=null;
	public String name=null;
	public String no=null;
	public String groupcode=null;
	public String groupname=null;

	public void setCode(String sCode)
	{
		code=sCode;
	}
	
	public void setCardno(String sCardno)
	{
		cardno=sCardno;
	}
	
	public void setNo(String sNo)
	{
		no=sNo;
	}
	public void setGroupcode(String sGroupcode)
	{
		groupcode=sGroupcode;
	}
	public void setGroupname(String sGroupname)
	{
		groupname=sGroupname;
	}
	public void setName(String sName)
	{
		name=sName;
	}
}
